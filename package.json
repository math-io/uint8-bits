{
  "name": "math-uint8-bits",
  "version": "1.0.0",
  "description": "Returns a string giving the literal bit representation of an unsigned 8-bit integer.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "if [ \"${TRAVIS}\" ]; then npm run test-ci; else npm run test-local; fi",
    "test-local": "tape \"./test/*.js\" | tap-spec",
    "test-ci": "npm run test-local && xvfb-run npm run test-browsers",
    "test-cov": "istanbul cover --dir ./reports/coverage --report lcov tape -- \"./test/*.js\"",
    "test-browsers": "browserify ./test/*.js | testling | tap-spec",
    "coverage": "istanbul cover --dir ./reports/codecov/coverage --report lcovonly tape -- \"./test/*.js\" && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/math-io/uint8-bits.git"
  },
  "keywords": [
    "compute.io",
    "compute-io",
    "compute",
    "computation",
    "math.io",
    "math-io",
    "math",
    "mathematics",
    "number",
    "binary",
    "bin",
    "bits",
    "uint8",
    "nonnegative",
    "unsigned",
    "integer",
    "int",
    "8-bit",
    "tostring",
    "string"
  ],
  "bugs": {
    "url": "https://github.com/math-io/uint8-bits/issues"
  },
  "dependencies": {
    "const-max-uint8": "^1.0.2",
    "math-floor": "^1.0.1",
    "utils-left-pad-string": "^1.0.1"
  },
  "devDependencies": {
    "browserify": "13.x.x",
    "codecov": "3.x.x",
    "istanbul": "^0.4.1",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x",
    "testling": "1.x.x"
  },
  "testling": {
    "files": [
      "test/*.js"
    ],
    "browsers": [
      "iexplore/6.0..latest",
      "firefox/3.0..latest",
      "firefox/nightly",
      "chrome/4.0..latest",
      "chrome/canary",
      "opera/10.0..latest",
      "opera/next",
      "safari/4.0..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "license": "MIT"
}
